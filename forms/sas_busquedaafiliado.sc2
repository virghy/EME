	PROCEDURE crearconsulta
		LPARAMETERS lcCadena,lcCampo,lcValor,lcCondicion
		IF !EMPTY(lcValor)
			IF !EMPTY(lcCadena)
				lcCadena = lcCadena + " AND "
			ENDIF
			
			lcCadena = lcCadena + lcCampo + lcCondicion + " "
			IF lcCondicion ='like'
				lcCadena = lcCadena + "'%"+lcValor+"%'"
			ELSE
				lcCadena = lcCadena + "'"+ TRANSFORM(lcValor)+"'"
			ENDIF
		ENDIF
		RETURN lcCadena
		
			
	ENDPROC

	PROCEDURE Init
		Parameter LNPOSICION, LCPARAMETRO
		
		IF PCOUNT()>0
			this.WindowType= 1
		ENDIF
			
	ENDPROC

	PROCEDURE Unload
		RETURN this.Tag
	ENDPROC

	PROCEDURE cmdBuscar.Click
		LOCAL lcSQL
		
		lcSQL=""
		
		*lcSQL= cadenaConsulta(lcSQL,"c.",ALLTRIM(thisform.txtnombre.Value),"LIKE")
		lcSQL= cadenaConsulta(lcSQL,"a.Nombre",ALLTRIM(thisform.txtnombre.Value),"LIKE")
		lcSQL= cadenaConsulta(lcSQL,"a.CI",ALLTRIM(thisform.txtCI.Value),"=")
		lcSQL= cadenaConsulta(lcSQL,"a.Direccion",ALLTRIM(thisform.txtdireccion.Value),"LIKE")
		lcSQL= cadenaConsulta(lcSQL,"a.Telefono",ALLTRIM(thisform.txttelefono.Value),"LIKE")
		lcSQL= cadenaConsulta(lcSQL,"c.NroContrato",ALLTRIM(thisform.txtcontrato.Value),"LIKE")
		lcSQL= cadenaConsulta(lcSQL,"isnull(cl.RazSocial,cl.Nombre)",ALLTRIM(thisform.txtempresa.Value),"LIKE")
		
			
		
		*MESSAGEBOX(lcSQL)
		IF EMPTY(lcSQL)
			MESSAGEBOX("Ingrese al menos un valor para buscar",64,TASTRADE_LOC)
			RETURN
		ENDIF
		
		
		IF this.Parent.tsestado.Value=1
			lcSQL=cadenaConsulta(lcSQL,"C.IdEstado",'A',"=")
			lcSQL=cadenaConsulta(lcSQL,"C.IdEstado",'A',"=")			
		ENDIF
		
		IF this.Parent.tsestado.Value=2
			lcSQL=cadenaConsulta(lcSQL,"C.IdEstado",'I',"=")
			lcSQL=cadenaConsulta(lcSQL,"C.IdEstado",'I',"=")			
		ENDIF
		
			
		
		TEXT TO cmdsql NOSHOW TEXTMERGE  
			SELECT DISTINCT c.IdContrato, c.NroContrato, isnull(cl.RazSocial,cl.Nombre) Titular, a.Nombre, a.CI, a.Direccion, a.Telefono, 
			CONVERT(CHAR(10), a.FechaNac,105) FechaNac, c.IdEstado,ac.IdEstado EAF, a.IdAfiliado
			FROM            sas_Contrato AS c left JOIN
			                         sas_AfiliadoContrato AS ac ON c.IdContrato = ac.IdContrato left JOIN
			                         sas_afiliados AS a ON ac.IdEmpresa = a.IdEmpresa AND ac.IdAfiliado = a.IdAfiliado
			                         inner join vt_Clientes cl on c.IdEmpresa=cl.IdEmpresa and c.IdCliente = cl.IdCliente 
			where c.IdEmpresa=?oApp.Empresa AND <<lcSQL>>
			ORDER BY c.NroContrato, a.Nombre
		ENDTEXT
		thisform.tsgrid1.RecordSource=""
		thisform.runsql(cmdSQL,'_Result')
		
		thisform.tsgrid1.RecordSourceType= 1
		thisform.tsgrid1.RecordSource='_Result'
		
		
		
		
	ENDPROC

	PROCEDURE Tscommandbutton1.Click
		thisform.Tag=_Result.NroContrato +'|' + NVL(_Result.IdAfiliado,'')
		*thisform.retorna2= _Result.IdAfiliado
		thisform.Release()
	ENDPROC

	PROCEDURE Tscommandbutton2.Click
		thisform.Tag=''
		thisform.Release()
	ENDPROC

