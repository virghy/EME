			CASE BAR()=2
		*		thisform.SetFont
			CASE BAR()=3
		*		DO FORM sas_Servicios WITH 0,'0000'
			CASE BAR()=5
		*		thisform.Release
		ENDCASE
		
		RETURN 
		
		
	ENDPROC

ENDDEFINE
