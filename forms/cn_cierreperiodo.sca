[ RECORD]
[PLATFORM] WINDOWS 
[UNIQUEID] _1R413Q0KK
[CLASS] tscommandbutton
[CLASSLOC] ..\libs\tsbase.vcx
[BASECLASS] commandbutton
[OBJNAME] cmdOk
[PARENT] Tsbaseform12.Pageframe1.Page2
[START PROPERTIES]
Top = 220
Left = 198
Height = 23
Width = 76
Caption = "\<Aceptar"
ZOrderSet = 5
Name = "cmdOk"
[END PROPERTIES]
[START METHODS]
PROCEDURE Click
TEXT TO CMDSQL NOSHOW
BEGIN TRANSACTION
	UPDATE cn_Asientos
		SET nro_Asiento = 0,
		procesado = 0
	where idEmpresa = ?oApp.Empresa
		and Ejercicio = ?oApp.Ejercicio
		and MONTH(fecha)= ?oApp.Periodo;
	IF 	@@ERROR <> 0
		begin
			ROLLBACK
			RAISERROR('Ha ocurrido un error al revertir el cierre del periodo',16,1)
			RETURN
		END
	UPDATE Ejercicios
		SET UltimoCierre = case when ?oApp.Periodo = 1 then 13 else ?oApp.Periodo-1 end
		where idEmpresa = ?oApp.Empresa
			and Ejercicio = ?oApp.Ejercicio
	IF 	@@ERROR <> 0
		begin
			ROLLBACK
			RAISERROR('Ha ocurrido un error al revertir el cierre del periodo',16,1)
			RETURN
		END
	ELSE
		commit
ENDTEXT
IF SQL(CMDSQL, 'Cursor')>0
MESSAGEBOX('La reversion del cierre de periodo se realizó correctamente', 64, "Futura Software")
THISFORM.RELEASE()
ENDIF
ENDPROC
[END METHODS]

