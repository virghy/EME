FUNCTION validar
lcalias = ALIAS()
IF EMPTY(vendedor.idcomision)
     MESSAGEBOX("El vendedor seleccionado no tiene asignado una Tabla de Comisiones", 64,  ;
               "Sistema de Gestión Financiera")
     RETURN .F.
ENDIF
IF EMPTY(creditos.cuotas)
     MESSAGEBOX("Debe indicar una Cuota.", 64, "Sistema de Gestión Financiera")
     RETURN .F.
ENDIF
SELECT TOP 1 comision FROM datos!fn_comisiones_det a WHERE a.idcomision = vendedor.idcomision AND a.cuota <=  ;
       creditos.cuotas ORDER BY comision INTO CURSOR xComision
SELECT (lcalias)
IF RECCOUNT('xComision') = 0
     MESSAGEBOX('No se ha encontrado la comision correspondiente a esta cuota.', 64,  ;
               "Sistema de Gestión Financiera")
     RETURN .F.
ENDIF
IF EMPTY(creditos.comision)
     REPLACE comision WITH xcomision.comision IN creditos
ELSE
     IF xcomision.comision <> creditos.comision
          msg = MESSAGEBOX("La comision asignada a este credito es diferente a la actual" + CHR(13) +  ;
                "Desea actualizarla?", 068, "Sistema de Gestión Financiera")
          IF msg = 6
               REPLACE comision WITH xcomision.comision IN creditos
          ENDIF
     ENDIF
ENDIF
ENDFUNC
**
PROCEDURE save
LOCAL lccredito, lncuota, ccuota, lnimpcuota, vencimiento, pvencimiento, llcambio
SELECT creditos
llcambio = OLDVAL('cuotas', 'creditos') <> creditos.cuotas .OR. OLDVAL('ImporteCuota', 'creditos') <>  ;
           creditos.importecuota .OR. OLDVAL('PrimerVto', 'creditos') <> creditos.primervto .OR.  .NOT.  ;
           thisform.isnewandempty()
IF llcambio
     DELETE IN cuotas ALL FOR creditos.idcredito = cuotas.idcredito
     lccredito = creditos.idcredito
     ccuota = creditos.cuotas
     lnimpcuota = creditos.importecuota
     vencimiento = creditos.primervto
     lncuota = 0
     lnacum = 0
     FOR lncuota = 1 TO ccuota
          IF lnacum + lnimpcuota > creditos.total
               INSERT INTO cuotas (idcredito, nrocuota, monto, vence, saldo) VALUES (lccredito, lncuota,  ;
                      creditos.total - lnacum, vencimiento, creditos.total - lnacum)
          ELSE
               INSERT INTO cuotas (idcredito, nrocuota, monto, vence, saldo) VALUES (lccredito, lncuota,  ;
                      lnimpcuota, vencimiento, lnimpcuota)
          ENDIF
          lnacum = lnacum + lnimpcuota
          vencimiento = GOMONTH(creditos.primervto, lncuota)
     ENDFOR
ENDIF
DODEFAULT()
ENDPROC
