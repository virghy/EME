PROCEDURE Refresh
IF  .NOT. THISFORM.EDITMODE
M.IDCLIENTE = EV_EVENTOS.IDCLIENTE
REQUERY('bs_clientes')
ENDIF
DODEFAULT()
ENDPROC
PROCEDURE afteraddnew
SELECT EV_EVENTOSOP
THISFORM.PAGEFRAME1.PAGE1.PAGEFRAME1.PAGE2.REFRESH()
THISFORM.PAGEFRAME1.PAGE1.PAGEFRAME1.PAGE3.REFRESH()
TEXT TO CMDSQL NOSHOW

	SELECT     IdProducto, Descripcion, Tipo='P',Familia,Precio, Costo=Ult_Costo,
				Calcular=Convert(bit,Case when ISNULL(ev_Incluye,0)=0 AND ISNULL(ev_CalcularCierre,0)=0 then 1 else 0 end),
				CalcularCierre=ev_CalcularCierre,Ev_Incluye,Ev_Presentar
	FROM         dbo.st_Producto
	where idEmpresa = ?oApp.Empresa and Activo = 1 and Idtipo in ('P','S')
	union 
	Select IdProducto=IdFamilia,Descripcion,Tipo='G',Familia=IdFamilia,Precio=0,Costo=0,
			Calcular=convert(bit,0),CalcularCierre=convert(bit,0),Ev_Incluye=convert(bit,0),Ev_Presentar=convert(bit,1)
	from st_familia where idempresa=?oApp.Empresa
	order by 4,3,2
ENDTEXT
THIS.RUNSQL(CMDSQL, 'cOpcion')
SELECT COPCION
SCAN
INSERT INTO ev_EventosOp (IDEMPRESA, IDEVENTO, IDPRODUCTO, DESCRIPCION, TIPO, PRECIO, CALCULAR, CALCULARCIERRE, INCLUYE, PRESENTAR, COSTO, SEL) VALUES (OAPP.EMPRESA, EV_EVENTOS.IDEVENTO, COPCION.IDPRODUCTO, COPCION.DESCRIPCION, COPCION.TIPO, COPCION.PRECIO, COPCION.CALCULAR, COPCION.CALCULARCIERRE, COPCION.EV_INCLUYE, COPCION.EV_PRESENTAR, COPCION.COSTO, IIF(COPCION.TIPO='P', .F., .NULL.))
ENDSCAN
SET FILTER TO PRESENTAR=.T. IN EV_EVENTOSOP
APPEND IN BS_CLIENTES BLANK
ENDPROC
PROCEDURE calcular
LOCAL M.ADICIONALES
M.ADICIONALES = 0
SELECT EV_EVENTOSOP
SUM PRECIO TO M.ADICIONALES FOR SEL=.T. .AND. CALCULAR=.T.
REPLACE ADICIONALUNITARIO WITH COPCIONES.COSTOADICIONAL IN EV_EVENTOS
THISFORM.PAGEFRAME1.PAGE1.PAGEFRAME1.PAGE3.SERVICIOS.VALUE = M.ADICIONALES
THISFORM.PAGEFRAME1.PAGE1.PAGEFRAME1.PAGE3.TXTCOSTOADICIONAL.VALUE = COPCIONES.COSTOADICIONAL*EV_EVENTOS.NROADICIONAL
THISFORM.PAGEFRAME1.PAGE1.PAGEFRAME1.PAGE3.TXTCOSTOBASICO.VALUE = COPCIONES.IMPORTE
THISFORM.PAGEFRAME1.PAGE1.PAGEFRAME1.PAGE3.TXTTOTAL.VALUE = M.ADICIONALES+COPCIONES.IMPORTE
THISFORM.PAGEFRAME1.PAGE1.PAGEFRAME1.PAGE3.TXTSALDO.REFRESH()
ENDPROC
PROCEDURE delete
IF  .NOT. EMPTY(EV_EVENTOS.IDFACTURA)
IF MESSAGEBOX("El evento que desea eliminar ya fue facturado. "+"Desea eliminar de todos modos?", 32, "Futura Software")=32
RETURN DODEFAULT()
ELSE
RETURN .F.
ENDIF
ENDIF
DODEFAULT()
ENDPROC
PROCEDURE lallowedits_access
RETURN ISNULL(EV_EVENTOS.ESTADO) .OR. EMPTY(EV_EVENTOS.ESTADO)
ENDPROC
PROCEDURE validar
IF EMPTY(EV_EVENTOS.IDCLIENTE)
	LCULTNRO = THIS.CAMPONRO
	THIS.CAMPONRO = "idcliente,vt_clientes,idempresa=?oApp.Empresa"
	THIS.CAMPOTAM = 4
	REPLACE EV_EVENTOS.IDCLIENTE WITH THISFORM.ULTIMONRO IN EV_EVENTOS
	REPLACE BS_CLIENTES.IDCLIENTE WITH EV_EVENTOS.IDCLIENTE, BS_CLIENTES.IDEMPRESA WITH OAPP.EMPRESA IN BS_CLIENTES
	THIS.CAMPONRO = LCULTNRO
	THIS.CAMPOTAM = 0
	= TABLEUPDATE(.T., .T., 'bs_Clientes')
ENDIF
IF ALLTRIM(EV_EVENTOS.DHORA)=":" .OR. EMPTY(EV_EVENTOS.DHORA)
	REPLACE DHORA WITH .NULL. IN EV_EVENTOS
ENDIF
IF ALLTRIM(EV_EVENTOS.HHORA)=":" .OR. EMPTY(EV_EVENTOS.HHORA)
	REPLACE HHORA WITH .NULL. IN EV_EVENTOS
ENDIF
IF  .NOT. ISNULL(EV_EVENTOS.DHORA) .AND.  .NOT. ISNULL(EV_EVENTOS.HHORA)
	THIS.DFECHA = DTOC(EV_EVENTOS.FECHA)+" "+EV_EVENTOS.DHORA
	THIS.HFECHA = DTOC(EV_EVENTOS.FECHA)+" "+EV_EVENTOS.HHORA
	TEXT TO CMDSQL NOSHOW
		SELECT NroEvento FROM ev_Eventos
		WHERE dHora is NOT null AND hHora is NOT null
		and (?this.dFecha between convert(datetime,convert(char(10),Fecha,105) + ' ' + dHora)
		and  convert(datetime,convert(char(10),Fecha,105) + ' ' + hHora)
		OR ?this.hFecha between convert(datetime,convert(char(10),Fecha,105) + ' ' + dHora)
		and  convert(datetime,convert(char(10),Fecha,105) + ' ' + hHora))
		and IdEmpresa=?oApp.Empresa
		and NroEvento<> ?ev_eventos.NroEvento
	ENDTEXT
	THIS.RUNSQL(CMDSQL, 'eRepetido')
	IF RECCOUNT('eRepetido')>0
		MESSAGEBOX("Existe otro evento que coincide con la hora de este evento."+CHR(13)+"Evento Nro. "+STR(EREPETIDO.NROEVENTO), 64, "Futura Software")
		RETURN .F.
	ENDIF
ELSE
ENDIF
THISFORM.PAGEFRAME1.PAGE1.PAGEFRAME1.PAGE3.TXTTOTALVALOR.REFRESH()
IF EV_EVENTOS.ADELANDO>0 .AND. THISFORM.PAGEFRAME1.PAGE1.PAGEFRAME1.PAGE3.TXTTOTALVALOR.VALUE=0
	INSERT INTO ev_EventosVal (IDEVENTO, IDEMPRESA, IDTIPOVALOR, IMPORTE, IDMONEDA, CONCEPTO) VALUES (EV_EVENTOS.IDEVENTO, OAPP.EMPRESA, "1", EV_EVENTOS.ADELANDO, 'GS', 'Seña')
ENDIF
THISFORM.PAGEFRAME1.PAGE1.PAGEFRAME1.PAGE3.TXTTOTALVALOR.REFRESH()
IF THISFORM.PAGEFRAME1.PAGE1.PAGEFRAME1.PAGE3.TXTTOTALVALOR.VALUE<>EV_EVENTOS.ADELANDO
	MESSAGEBOX("Existe una diferencia entre la suma de los Valores y el importe del Adelanto", 64, "Futura Software")
	RETURN .F.
ENDIF
LNDIA = DOW(EV_EVENTOS.FECHA, 1)
TEXT TO CMDSQL NOSHOW
	SELECT COUNT(*) As Cantidad FROM ev_feriados
		WHERE dia=datepart(dd,?ev_Eventos.Fecha)
			  and mes=datepart(mm,?ev_Eventos.Fecha)
			  and (año=datepart(yy,?ev_Eventos.Fecha) or Anual = 1)

ENDTEXT
THIS.RUNSQL(CMDSQL, 'cFeriados')
SELECT EV_EVENTOSOP
SET FILTER TO
SCAN FOR ALLTRIM(IDPRODUCTO)='000019'
	IF CFERIADOS.CANTIDAD>0 .OR. INLIST(LNDIA, 1, 7)
		REPLACE SEL WITH .T., CANTIDAD WITH 1 FOR ALLTRIM(IDPRODUCTO)='000019' IN EV_EVENTOSOP
	ELSE
		REPLACE SEL WITH .F., CANTIDAD WITH 0 FOR ALLTRIM(IDPRODUCTO)='000019' IN EV_EVENTOSOP
	ENDIF
ENDSCAN
SET FILTER TO PRESENTAR=.T. IN EV_EVENTOSOP
ENDPROC
