[START METHODS]
PROCEDURE Init
CREATE CURSOR cSeleccion (IDPRODUCTO C (20), DESCRIPCION C (50), SEL L, TIPO C (1))
THISFORM.PAGEFRAME1.PAGE1.PAGEFRAME1.PAGE2.GRDOPCIONES.RECORDSOURCETYPE = 1
THISFORM.PAGEFRAME1.PAGE1.PAGEFRAME1.PAGE2.GRDOPCIONES.RECORDSOURCE = 'ev_EventosOp'
THISFORM.PAGEFRAME1.PAGE1.PAGEFRAME1.PAGE2.GRDOPCIONES.COLUMN1.CONTROLSOURCE = 'ev_EventosOp.Sel'
THISFORM.PAGEFRAME1.PAGE1.PAGEFRAME1.PAGE2.GRDOPCIONES.COLUMN2.CONTROLSOURCE = 'ev_EventosOp.Descripcion'
THISFORM.PAGEFRAME1.PAGE1.PAGEFRAME1.PAGE2.GRDOPCIONES.COLUMN2.ENABLED = .F.
THISFORM.PAGEFRAME1.PAGE1.PAGEFRAME1.PAGE2.GRDOPCIONES.CANTIDAD.CONTROLSOURCE = 'ev_EventosOp.Cantidad'
DODEFAULT()
INSERT INTO cSeleccion (DESCRIPCION, TIPO) VALUES ('MENU INFANTIL', 'G')
INSERT INTO cSeleccion (DESCRIPCION, TIPO) VALUES ('Fun 1', 'P')
INSERT INTO cSeleccion (DESCRIPCION, TIPO) VALUES ('Fun 2', 'P')
INSERT INTO cSeleccion (DESCRIPCION, TIPO) VALUES ('Fun 3', 'P')
INSERT INTO cSeleccion (DESCRIPCION, TIPO) VALUES ('MENU ADULTO', 'G')
INSERT INTO cSeleccion (DESCRIPCION, TIPO) VALUES ('Lomito Simple', 'P')
INSERT INTO cSeleccion (DESCRIPCION, TIPO) VALUES ('Lomito Especial', 'P')
INSERT INTO cSeleccion (DESCRIPCION, TIPO) VALUES ('Hamburguesa Simple', 'P')
ENDPROC
