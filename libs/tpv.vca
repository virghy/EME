PROCEDURE formapago
Parameter LNIMPORTE
Local FRMCONDICION As Form, LCCONDICION, LCTIPOBASE, FRM, LODATA
FRMCONDICION = Createobject('condicionVenta')
FRMCONDICION.Show()
LCCONDICION = FRMCONDICION.VALOR
If Empty(LCCONDICION)
	Return .F.
Endif
LCTIPOBASE = Substr(LCCONDICION, At(",", LCCONDICION)+1, 1)
This.DESCUENTO = Val(Substr(LCCONDICION, At(",", LCCONDICION, 2)+1))
Do Case
Case LCTIPOBASE="1"
	Select VT_VALORES
	This.TABLA = Alias()
	Scatter Blank Name LODATA Fields IDEMPRESA, IDTIPOVALOR, NROCHEQUE, IDBANCO, IDMONEDA, FCHEMISION, FCHCHEQUE, IMPORTE, IDFACTURA
	LODATA.IMPORTE = LNIMPORTE
	LODATA.IDTIPOVALOR = "01"
	FRM = Createobject('condEfectivo', LODATA, This.DESCUENTO)
	FRM.IDCLIENTE_ASO.Value = This.IDCLIENTE
	FRM.Show()
Case LCTIPOBASE="2"
	Select VT_VALORES
	This.TABLA = Alias()
	Scatter Blank Name LODATA Fields IDEMPRESA, IDTIPOVALOR, NROCHEQUE, IDBANCO, IDMONEDA, FCHEMISION, FCHCHEQUE, IMPORTE, IDFACTURA
	LODATA.IMPORTE = LNIMPORTE
	LODATA.IDTIPOVALOR = "02"
	FRM = Createobject('condCheque', LODATA, This.DESCUENTO)
	FRM.IDCLIENTE_ASO1.Value = This.IDCLIENTE
	FRM.Show()
Case LCTIPOBASE="3"
	Select VT_VALORES
	This.TABLA = Alias()
	Scatter Blank Name LODATA Fields IDEMPRESA, IDTIPOVALOR, NROCHEQUE, IDBANCO, IDMONEDA, FCHEMISION, FCHCHEQUE, IMPORTE, IDFACTURA
	LODATA.IMPORTE = LNIMPORTE
	LODATA.IDTIPOVALOR = "03"
	FRM = Createobject('condTarjeta', LODATA, This.DESCUENTO)
	FRM.IDCLIENTE_ASO1.Value = This.IDCLIENTE
	FRM.Show()
Case LCTIPOBASE="4"
	Select VT_ORDENCOMPRA
	This.TABLA = Alias()
	Scatter Blank Name LODATA Fields IDEMPRESA, IDFACTURA, IDASOCIACION, IDSOCIO, CI, IMPORTE, CUOTAS, NROPAGARE, NROORDEN, FECHA
	LODATA.IMPORTE = LNIMPORTE
	FRM = Createobject('condAsociacion', LODATA, This.DESCUENTO)
	FRM.Show()
	this.idcondicion="CR"
Case LCTIPOBASE="5"
	Select VT_VALORES
	This.TABLA = ""
	Scatter Blank Name LODATA Fields IMPORTE, IDEMPRESA
	LODATA.IMPORTE = LNIMPORTE
	FRM = Createobject('condCredito', LODATA, This.DESCUENTO)
	FRM.Show()
	This.IDCONDICION = FRM.IDCONDICION
Endcase
LCCONDICION = FRM.VALOR
LODATA = FRM.Data
This.IDCLIENTE = FRM.IDCLIENTE
This.NOTAS = FRM.Tag
This.CLIENTE = FRM.Comment
Release FRM
If Empty(LCCONDICION)
	Return .F.
Else
	LODATA.IDEMPRESA = OAPP.EMPRESA
	This.Data = LODATA
	Release LODATA
	Return .T.
Endif

ENDPROC
