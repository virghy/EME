IF THIS.NROCOPIAS='0'
M.NROCOPIAS = 1
ELSE
M.NROCOPIAS = VAL(THIS.NROCOPIAS)
ENDIF
FOR I = 1 TO M.NROCOPIAS
RUN "Copy c:\factura.txt lpt1"
ENDFOR
