[START METHODS]
PROCEDURE DownClick
LOCAL ld
WITH THIS.PARENT
  ld = DATE(This.VALUE,MONTH(.VALUE),DAY(.VALUE))
  IF EMPTY(ld)
    ld = DATE(This.VALUE,MONTH(.VALUE),DAY(.VALUE)-1)
  ENDIF
  .VALUE = ld
ENDWITH
THIS.PARENT._Refresh()
ENDPROC
PROCEDURE Init
THIS.VALUE = YEAR(THIS.PARENT.VALUE)

ENDPROC
PROCEDURE UpClick
LOCAL ld
WITH THIS.PARENT
  ld = DATE(This.VALUE,MONTH(.VALUE),DAY(.VALUE))
  IF EMPTY(ld)
    ld = DATE(This.VALUE,MONTH(.VALUE),DAY(.VALUE)-1)
  ENDIF
  .VALUE = ld
ENDWITH
THIS.PARENT._Refresh()
ENDPROC
PROCEDURE Valid
LOCAL ld
WITH THIS.PARENT
  ld = DATE(This.VALUE,MONTH(.VALUE),DAY(.VALUE))
  IF EMPTY(ld)
    ld = DATE(This.VALUE,MONTH(.VALUE),DAY(.VALUE)-1)
  ENDIF
  .VALUE = ld
ENDWITH
THIS.PARENT._Refresh()
ENDPROC
[END METHODS]

