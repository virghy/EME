PROCEDURE InteractiveChange
LOCAL ld, ln
WITH THIS.PARENT
  ld = DATE(YEAR(.VALUE),THIS.VALUE,DAY(.VALUE))
  ln = 1
  DO WHILE EMPTY(ld)
    ld = DATE(YEAR(.VALUE),THIS.VALUE,DAY(.VALUE)-ln)
    ln = ln + 1
  ENDDO
  .VALUE = ld
ENDWITH
THIS.PARENT._Refresh()

ENDPROC
